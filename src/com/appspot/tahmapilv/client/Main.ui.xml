<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:my="urn:import:com.appspot.tahmapilv.util">
	<ui:style>
		html,body {
			height: 100%;
			margin: 0;
			padding: 0;
		}
		#body, .fullheight {
			height: 100%;
		}
		#menu {
			width: 150px;
			height: 100%;
			float: left;
			border-right: 1px solid black;
		}
		#content {
			float: left;
		}
		#content:after {
			clear: both;
		}
		.error {
			color: red;
		}
		.warning {
			color: orange;
		}
		.message {
			color: green;
		}
	</ui:style>
	<g:HTMLPanel styleName="{style.fullheight}">
		<div id="menu">
			<a href="#">Front Page</a><br/>
			<a href="#Capacitor">Capacitor</a><br/>
			<a href="#Inductor">Inductor</a>
		</div>
		<g:FlowPanel ui:field="content"></g:FlowPanel>
	</g:HTMLPanel>
</ui:UiBinder> 